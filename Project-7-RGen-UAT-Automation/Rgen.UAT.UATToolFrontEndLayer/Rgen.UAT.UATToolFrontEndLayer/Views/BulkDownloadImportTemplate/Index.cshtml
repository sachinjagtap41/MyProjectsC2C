            <option value="United States" title="United States">
                United States
            </option>
            <option value="Western Europe" title="Western Europe">
                Western Europe
            </option>
        </select>
    </div>

    <div class="pgHead" style="margin-left:5px">Bulk Upload Test Scripts</div>

    <div class="prjHead" id="projectName" style="display:none"></div>

    <table class="rgTableBread">
        <tr>
            <td id="groupTitle"><label class="rgBCTitle"></label></td>
            <td id="portfolioTitle"><label class="rgBCTitle"></label></td>
            <td id="projectTitle"><label class="rgBCTitle"></label></td>
            <td id="versionTitle"><label class="rgBCTitle"></label></td>
        </tr>
        <tr>
            <td id="groupHead" style="cursor:pointer">
                <label class="rgBCHead">
                    Group
                </label><div id="assoGroups" class="headDD" style="height:auto;-ms-overflow-y: auto; max-height: 400px;"><ul style="list-style: none;" id="grpul" class="headul"></ul></div>
            </td>
            <td id="portfolioHead" style="cursor:pointer">
                <label class="rgBCHead">
                    Portfolio
                </label><div id="assoPortfolios" class="headDD"><ul style="list-style: none;" id="portful" class="headul"></ul></div>
            </td>
            <td id="projectHead" style="cursor:pointer">
                <label class="rgBCHead">
                    Project
                </label><div id="assoProjects" class="headDD" style="-ms-overflow-y: auto; max-height: 400px;"><ul style="list-style: none;" id="prjctul" class="headul"></ul></div>
            </td>
            <td id="versionHead" style="cursor:pointer">
                <label class="rgBCHead">
                    Version
                </label><div id="assoVersions" class="headDD"><ul style="list-style: none;" id="versul" class="headul"></ul></div>
            </td>
        </tr>
    </table>



    <div id="pageTab" class="navTabs" style="margin-left:5px">
        <h2 style="width: 100px; color: rgb(0, 0, 0); text-transform: uppercase; font-size: 12px; float: left; cursor: pointer;">
            Upload
        </h2>
        <h2 style="width: 100px; color: rgb(127, 127, 127);  text-transform: uppercase; font-size: 12px; float: left; cursor: pointer;">
            History
        </h2>
    </div>

    <p class="clear"></p>


    <div id="impExpTemplate" style="margin-left: 5px;">
        <div id="divUpload"><input type="file" id="upload" style="display:none" /></div>
        <div style="width:450px">
            <div style="padding-bottom:10px;font-size:16px">
                Please use the
                downloaded template to import.
            </div>
                       
            @*<input type="button" id="btnDownloadTemplate" style="width:150px;height:35px;" class="btnNew1" value="Download Template" onclick="Main.showLoading(); setTimeout('impTS.exportExcel();', 200);" />&nbsp;
            <input id="btnUploadTemplate" type="button" style="width:150px;height:35px;" value="Upload Template" class="btnNew1" onclick="Main.showLoading(); setTimeout('impTS.importExcel();', 200);" />*@

            <input type="button" id="btnDownloadTemplate" style="width:150px;height:35px;" class="btnNew1" value="Download Template" />&nbsp;
            <input id="btnUploadTemplate" type="button" style="width:150px;height:35px;" value="Upload Template" class="btnNew1" />
 
              <input type="file" id="fileUpload" onchange="impTS.uploadFile();" style="display:none" />


        </div>
        <div id="noteDiv" style="margin-top:40px">
            <p style="font-size:16px;margin-bottom:6px">
                Please follow below
                steps to Import Test Scripts:
            </p>
            <p>&nbsp;&nbsp;1. Download a Template using &#39;Download Template&#39; button.</p>
            <p>
                &nbsp;&nbsp;2. Populate data of Test Passes, Testers, Test Cases and
                Test Scripts as directed in template.
            </p>
            <p>&nbsp;&nbsp;3. Save Template in local drive.</p>
            <p>
                &nbsp;&nbsp;4. Upload this saved template using &#39;Upload Template&#39;
                button.
            </p>

            <!--<p style="margin-top:8px">Download <a id="downloadPdf" style="color:#0066FF;text-decoration:underline; cursor:pointer" target='_blank' href="../SiteAssets/GuidelineDocs/Detailed Guidelines to Upload Tester Template.pdf">
            File</a> for detailed guidelines.</p>-->

        </div>
    </div>
    <div id="divHtml" style="display:none"></div>
    <div id="dvHistory" style="display:none;margin-left: 5px;">
        <div id="uploadHistoryGrid">
            <table class="tableGrid" style="width: 950px; margin-top: 8px; display: none;" cellSpacing="1">
                <thead>
                    <tr class="griDetails">
                        <td align="center" style="width: 5%;">#</td>
                        <td style="width: 35%;">FILE NAME</td>
                        <td style="width: 20%;">UPLOADED BY</td>
                        <td style="width: 15%;">UPLOADED DATE</td>
                        <td align="center" style="width: 10%; border-right-color: currentColor; border-right-width: medium; border-right-style: none;">
                            ACTION
                        </td>
                    </tr>
                    <tr>
                        <td class="gridHeadingRow" colSpan="6"></td>
                    </tr>
                    <tr><td colSpan="6"></td></tr>
                    <tr><td colSpan="6"></td></tr>
                    <tr><td colSpan="6"></td></tr>
                    <tr><td colSpan="6"></td></tr>
                </thead>
                <tbody id="showUploadHistory">&nbsp;</tbody>
            </table>
        </div>

        <h3 id="noP" style="color: rgb(184, 184, 184); font-size: 30px;">
            No
            Upload History available!
        </h3>

        <div id="Pagination1" style="margin-top: 20px; float: right; display: none;">
            <div class="pagination">
                <span class="current prev">Prev</span>
                <span class="current">1</span>
                <span class="current next">Next</span>
            </div>
        </div>

    </div>

    <div style="width:800px" id="errorMsg"></div>

</div>